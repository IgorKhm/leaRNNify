digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=black]
	3 [label=2 color=green]
	4 [label=3 color=green]
	5 [label=4 color=green]
	2 -> 3 [label=a]
	2 -> 4 [label="b,c,e"]
	2 -> 2 [label=d]
	4 -> 5 [label=a]
	4 -> 3 [label="b,c"]
	4 -> 1 [label=d]
	4 -> 4 [label=e]
	5 -> 2 [label=a]
	5 -> 3 [label="b,e"]
	5 -> 1 [label=c]
	5 -> 5 [label=d]
	1 -> 4 [label="a,e"]
	1 -> 2 [label="b,c"]
	1 -> 3 [label=d]
	3 -> 5 [label="a,e"]
	3 -> 2 [label="b,c"]
	3 -> 3 [label=d]
}
