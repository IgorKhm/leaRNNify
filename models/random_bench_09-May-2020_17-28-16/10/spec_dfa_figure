digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=black]
	3 [label=2 color=green]
	4 [label=3 color=green]
	5 [label=4 color=green]
	2 -> 1 [label="a,e"]
	2 -> 5 [label="b-d"]
	3 -> 1 [label=a]
	3 -> 3 [label="b,c"]
	3 -> 2 [label="d,e"]
	1 -> 3 [label="a,c,e"]
	1 -> 2 [label="b,d"]
	4 -> 3 [label="a,c-e"]
	4 -> 2 [label=b]
	5 -> 5 [label=a]
	5 -> 2 [label=b]
	5 -> 1 [label="c,e"]
	5 -> 4 [label=d]
}
