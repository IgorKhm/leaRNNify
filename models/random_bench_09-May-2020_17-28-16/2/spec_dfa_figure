digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=green]
	3 [label=2 color=black]
	4 [label=3 color=green]
	3 -> 4 [label="a,d"]
	3 -> 1 [label=b]
	3 -> 2 [label="c,e"]
	1 -> 2 [label="a,c,e"]
	1 -> 3 [label=b]
	1 -> 1 [label=d]
	2 -> 2 [label=a]
	2 -> 1 [label="b,e"]
	2 -> 4 [label="c,d"]
	4 -> 4 [label=a]
	4 -> 1 [label="b-d"]
	4 -> 3 [label=e]
}
