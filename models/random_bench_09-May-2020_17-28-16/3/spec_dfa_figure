digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=black]
	3 [label=2 color=green]
	4 [label=3 color=black]
	5 [label=4 color=green]
	5 -> 2 [label="a,d"]
	5 -> 1 [label=b]
	5 -> 5 [label=c]
	5 -> 4 [label=e]
	1 -> 3 [label="a,d"]
	1 -> 4 [label=b]
	1 -> 1 [label="c,e"]
	3 -> 5 [label=a]
	3 -> 4 [label=b]
	3 -> 1 [label="c,e"]
	3 -> 3 [label=d]
	2 -> 3 [label="a,d,e"]
	2 -> 4 [label=b]
	2 -> 5 [label=c]
	4 -> 4 [label=a]
	4 -> 5 [label=b]
	4 -> 1 [label="c-e"]
}
