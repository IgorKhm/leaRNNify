digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=green]
	3 [label=2 color=green]
	4 [label=3 color=black]
	4 -> 1 [label=a]
	4 -> 3 [label="b,e"]
	4 -> 4 [label="c,d"]
	2 -> 4 [label="a,d"]
	2 -> 1 [label="b,e"]
	2 -> 3 [label=c]
	3 -> 1 [label="a,b"]
	3 -> 2 [label=c]
	3 -> 3 [label=d]
	3 -> 4 [label=e]
	1 -> 1 [label=a]
	1 -> 2 [label=b]
	1 -> 4 [label="c,d"]
	1 -> 3 [label=e]
}
