digraph {
	1 [label=start color=black shape=hexagon]
	2 [label=1 color=green]
	3 [label=2 color=green]
	4 [label=3 color=green]
	1 -> 2 [label=a]
	1 -> 4 [label="b,d"]
	1 -> 1 [label=c]
	1 -> 3 [label=e]
	2 -> 4 [label="a-d"]
	2 -> 3 [label=e]
	4 -> 1 [label=a]
	4 -> 2 [label="b,c,e"]
	4 -> 3 [label=d]
	3 -> 1 [label=a]
	3 -> 4 [label="b,d"]
	3 -> 2 [label="c,e"]
}
