digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=green]
	3 [label=2 color=black]
	4 [label=3 color=green]
	3 -> 4 [label=a]
	3 -> 1 [label=b]
	3 -> 3 [label=c]
	3 -> 2 [label="d,e"]
	2 -> 2 [label=a]
	2 -> 1 [label=b]
	2 -> 4 [label=c]
	2 -> 3 [label="d,e"]
	1 -> 2 [label="a-c"]
	1 -> 1 [label=d]
	1 -> 3 [label=e]
	4 -> 2 [label=a]
	4 -> 3 [label="b,c"]
	4 -> 4 [label=d]
	4 -> 1 [label=e]
}
