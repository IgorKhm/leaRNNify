digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=black]
	3 [label=2 color=green]
	4 [label=3 color=black]
	5 [label=4 color=green]
	1 -> 5 [label="a,c"]
	1 -> 4 [label=b]
	1 -> 2 [label=d]
	1 -> 3 [label=e]
	4 -> 3 [label=a]
	4 -> 1 [label="b,d"]
	4 -> 2 [label=c]
	4 -> 4 [label=e]
	2 -> 4 [label=a]
	2 -> 2 [label=b]
	2 -> 1 [label=c]
	2 -> 5 [label="d,e"]
	5 -> 3 [label="a,b,e"]
	5 -> 4 [label=c]
	5 -> 1 [label=d]
	3 -> 4 [label="a,e"]
	3 -> 5 [label="b,d"]
	3 -> 2 [label=c]
}
