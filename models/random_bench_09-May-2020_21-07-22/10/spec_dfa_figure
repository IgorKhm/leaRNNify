digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=black]
	3 [label=2 color=green]
	4 [label=3 color=green]
	2 -> 3 [label=a]
	2 -> 2 [label="b,e"]
	2 -> 4 [label=c]
	2 -> 1 [label=d]
	3 -> 2 [label=a]
	3 -> 1 [label=b]
	3 -> 3 [label="c-e"]
	4 -> 1 [label="a,e"]
	4 -> 3 [label="b,c"]
	4 -> 4 [label=d]
	1 -> 3 [label="a,d,e"]
	1 -> 2 [label=b]
	1 -> 4 [label=c]
}
