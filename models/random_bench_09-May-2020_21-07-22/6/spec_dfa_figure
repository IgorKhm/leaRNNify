digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=black]
	3 [label=2 color=green]
	4 [label=3 color=green]
	5 [label=4 color=green]
	2 -> 1 [label=a]
	2 -> 5 [label="b,c,e"]
	2 -> 4 [label=d]
	1 -> 5 [label="a,c,e"]
	1 -> 1 [label=b]
	1 -> 2 [label=d]
	4 -> 1 [label=a]
	4 -> 4 [label=b]
	4 -> 3 [label="c,d"]
	4 -> 5 [label=e]
	3 -> 5 [label="a,c"]
	3 -> 4 [label="b,d"]
	3 -> 3 [label=e]
	5 -> 5 [label="a-d"]
	5 -> 1 [label=e]
}
