digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=green]
	3 [label=2 color=green]
	4 [label=3 color=black]
	5 [label=4 color=green]
	4 -> 5 [label=a]
	4 -> 1 [label=b]
	4 -> 4 [label=c]
	4 -> 2 [label=d]
	4 -> 3 [label=e]
	3 -> 4 [label=a]
	3 -> 2 [label="b,c,e"]
	3 -> 1 [label=d]
	5 -> 2 [label="a,d"]
	5 -> 1 [label="b,e"]
	5 -> 5 [label=c]
	1 -> 3 [label="a,d"]
	1 -> 1 [label=b]
	1 -> 5 [label="c,e"]
	2 -> 3 [label="a,b"]
	2 -> 5 [label="c-e"]
}
