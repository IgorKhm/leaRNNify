digraph {
	1 [label=start color=black shape=hexagon]
	2 [label=1 color=green]
	3 [label=2 color=black]
	4 [label=3 color=green]
	5 [label=4 color=green]
	3 -> 5 [label=a]
	3 -> 3 [label="b,d,e"]
	3 -> 4 [label=c]
	1 -> 3 [label="a,d"]
	1 -> 2 [label="b,e"]
	1 -> 5 [label=c]
	5 -> 2 [label=a]
	5 -> 1 [label="b-e"]
	4 -> 1 [label=a]
	4 -> 5 [label=b]
	4 -> 2 [label=c]
	4 -> 3 [label=d]
	4 -> 4 [label=e]
	2 -> 3 [label="a,c"]
	2 -> 2 [label="b,d,e"]
}
