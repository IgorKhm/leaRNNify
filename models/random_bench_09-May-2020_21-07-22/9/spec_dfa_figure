digraph {
	1 [label=start color=black shape=hexagon]
	2 [label=1 color=green]
	3 [label=2 color=green]
	4 [label=3 color=green]
	5 [label=4 color=green]
	1 -> 1 [label="a,d"]
	1 -> 3 [label=b]
	1 -> 5 [label=c]
	1 -> 2 [label=e]
	5 -> 1 [label="a,d,e"]
	5 -> 2 [label="b,c"]
	3 -> 2 [label=a]
	3 -> 1 [label=b]
	3 -> 5 [label=c]
	3 -> 3 [label="d,e"]
	4 -> 3 [label="a,b"]
	4 -> 2 [label=c]
	4 -> 1 [label=d]
	4 -> 5 [label=e]
	2 -> 5 [label=a]
	2 -> 3 [label="b,e"]
	2 -> 4 [label=c]
	2 -> 1 [label=d]
}
