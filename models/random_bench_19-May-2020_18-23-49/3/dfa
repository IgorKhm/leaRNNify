digraph {
	1 [label=start color=black shape=hexagon]
	2 [label=1 color=black]
	3 [label=2 color=black]
	4 [label=3 color=black]
	5 [label=4 color=green]
	5 -> 1 [label="a,d"]
	5 -> 3 [label=b]
	5 -> 5 [label=c]
	5 -> 4 [label=e]
	3 -> 5 [label="a,b,e"]
	3 -> 3 [label=c]
	3 -> 4 [label=d]
	2 -> 5 [label="a,d"]
	2 -> 1 [label=b]
	2 -> 3 [label="c,e"]
	4 -> 1 [label=a]
	4 -> 2 [label=b]
	4 -> 5 [label="c,e"]
	4 -> 4 [label=d]
	1 -> 1 [label="a,b,e"]
	1 -> 2 [label="c,d"]
}
