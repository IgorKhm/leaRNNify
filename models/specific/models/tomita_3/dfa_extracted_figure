digraph {
	1 [label=start color=green shape=hexagon]
	2 [label=1 color=black]
	3 [label=2 color=green]
	4 [label=3 color=green]
	5 [label=4 color=black]
	1 -> 1 [label=0]
	1 -> 3 [label=1]
	2 -> 4 [label=0]
	2 -> 5 [label=1]
	5 -> 5 [label="0,1"]
	3 -> 2 [label=0]
	3 -> 1 [label=1]
	4 -> 2 [label=0]
	4 -> 4 [label=1]
}
